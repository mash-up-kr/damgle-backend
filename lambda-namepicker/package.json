{
  "name": "lambda-namepicker",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mash-up-kr/damgle-backend.git",
    "directory": "lambda-namepicker"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json --noEmit",
    "test": "NODE_ENV=test jest",
    "lint": "eslint . --ext .js,.ts",
    "dev": "ts-node src/entry.local.ts",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "start:debug": "node --require ts-node/register --inspect-brk src/entry.local.ts",
    "log:show": "aws logs tail /aws/lambda/namepicker --since 60m"
  },
  "author": {
    "name": "Karl Saehun Chung",
    "email": "nycom13@gmail.com"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "esbuild": "^0.14.43",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "prettier": "^2.6.0",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@damgle/utils": "workspace:*",
    "@fastify/aws-lambda": "^3.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-fastify": "^8.4.6",
    "fastify": "^3.29.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "eslintIgnore": [
    "*.js",
    "dist"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "transform": {
      "^.+.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "preset": "ts-jest"
  }
}
